{
    "name": "Twitch gift autoclick",
    "description": "Automatic click to +50 gift coin",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["*://www.twitch.tv/*", "*://twitch.tv/*"],
            "js": ["inject.js"]
        }
    ]
}
