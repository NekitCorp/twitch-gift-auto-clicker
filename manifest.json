{
    "name": "Twitch gift autoclick",
    "version": "0.4",
    "manifest_version": 2,
    "description": "Automatic click to +50 gift coin",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["*://www.twitch.tv/*", "*://twitch.tv/*"],
            "js": ["inject.js"]
        }
    ]
}
